    <script>
    function initialize() {
      var myLatlng = new google.maps.LatLng(39.7643324,-86.1591664);
      var mapOptions = {
      zoom: 18,
      center: myLatlng
    }
    var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

    var marker = new google.maps.Marker({
      position: myLatlng,
      map: map,
      title: 'Hello World!'
    });
  }

  google.maps.event.addDomListener(window, 'load', initialize);

  </script>
